# Database Configuration
# Set DB_TYPE to either 'mongodb' or 'sqlite'
DB_TYPE=mongodb

# MongoDB Configuration (only needed if DB_TYPE=mongodb)
MONGO_USER=
MONGO_PASSWORD=
MONGO_CLUSTER=
MONGO_DATABASE=Cluster0

# SQLite Configuration (only needed if DB_TYPE=sqlite)
# Leave empty to use default: ./todoapp.sqlite
SQLITE_PATH=

# Server Configuration
PORT=5050